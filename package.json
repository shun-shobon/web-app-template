{
  "name": "web-test-template",
  "version": "0.0.0",
  "description": "Project template for web testing",
  "author": "Shuntaro Nishizawa <me@shun.technology> (https://github.com/shun-shobon)",
  "license": "MIT",
  "private": true,
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "serve": "vite serve",
    "check": "run-p check:*",
    "check:type": "tsc",
    "check:format": "prettier --check --ignore-path .gitignore \"**/*.{js,jsx,ts,tsx,html,css,sass,scss}\"",
    "fix": "run-p fix:*",
    "fix:format": "prettier --write --ignore-path .gitignore \"**/*.{js,jsx,ts,tsx,html,css,sass,scss}\""
  },
  "devDependencies": {
    "npm-run-all": "^4.1.5",
    "prettier": "^2.3.0",
    "typescript": "^4.3.2",
    "vite": "^2.3.4",
    "vite-plugin-html": "^2.0.7"
  }
}
